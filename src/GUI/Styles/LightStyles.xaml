<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

  <SolidColorBrush x:Key="AccentBrush" Color="PLACEHOLDER" />
  <SolidColorBrush x:Key="TextBrush" Color="#000000" />
  <SolidColorBrush x:Key="InactiveTextBrush" Color="#5E5E5E" />
  <SolidColorBrush x:Key="DisabledTextBrush" Color="#9D9D9D" />
  <SolidColorBrush x:Key="BackgroundBrush" Color="#FFFFFF" />
  <SolidColorBrush x:Key="BackgroundElevatedBrush" Color="#FFFFFF" Opacity="0.65" />
  <SolidColorBrush x:Key="ButtonBackgroundBrush" Color="#FFFFFF" Opacity="0.5" />
  <SolidColorBrush x:Key="DisabledButtonBackgroundBrush" Color="#FFFFFF" Opacity="0.3" />
  <SolidColorBrush x:Key="ActiveButtonBackgroundBrush" Color="#000000" Opacity="0.02" />
  <SolidColorBrush x:Key="ControlBorderBrush" Color="#000000" Opacity="0.10" />
  <SolidColorBrush x:Key="ScrollBarBackgroundBrush" Color="#A0A0A0" />
  <SolidColorBrush x:Key="ToolTipBackgroundBrush" Color="#FFFFFF" />
  <SolidColorBrush x:Key="CheckBoxBorderBrush" Color="#858585" />
  <SolidColorBrush x:Key="GreenBrush" Color="#0F7B0F" />

</ResourceDictionary>
